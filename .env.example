# Solana Configuration
SOLANA_CLUSTER=devnet
RPC_URL=https://api.devnet.solana.com

# Treasury wallet (devnet only - NEVER use mainnet keys!)
AKCA_TREASURY_PUBLIC_KEY=YOUR_DEVNET_PUBKEY_HERE
AKCA_TREASURY_SECRET_KEY=YOUR_DEVNET_SECRET_KEY_BASE58_HERE

# NFT Collection (optional, can be empty for demo)
COLLECTION_MINT=

# VPN Configuration
DEMO_VPN_KEY_BASE64=YOUR_RANDOM_32_BYTES_BASE64_HERE

# WireGuard SSH Configuration (for remote server management)
WG_SSH_KEY_PATH=/path/to/your/ssh/private/key
# Alternative: SSH_KEY_PATH=/path/to/your/ssh/private/key

# Admin Authentication
ADMIN_KEY=your_secure_admin_key_here

# API Server
PORT=3000

# Generate DEMO_VPN_KEY_BASE64 with:
# node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"
